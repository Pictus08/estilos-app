{
    "name":  "@pictus/ui",
    "version":  "0.1.0",
    "description":  "UI library components for Next.js projects (React + Tailwind).",
    "main":  "dist/index.js",
    "module":  "dist/index.mjs",
    "types":  "dist/index.d.ts",
    "exports":  {
                    ".":  {
                              "require":  "./dist/index.js",
                              "types":  "./dist/index.d.ts",
                              "import":  "./dist/index.mjs"
                          }
                },
    "files":  [
                  "dist"
              ],
    "scripts":  {
                    "build":  "tsup src/index.ts --format esm,cjs --dts",
                    "dev":  "tsup src/index.ts --format esm,cjs --dts --watch",
                    "test":  "echo \"No tests yet\"",
                    "prepare":  "npm run build"
                },
    "keywords":  [
                     "react",
                     "ui",
                     "components",
                     "tailwind",
                     "nextjs"
                 ],
    "author":  "",
    "license":  "ISC",
    "type":  "commonjs",
    "dependencies":  {
                         "react":  "^19.1.1",
                         "react-dom":  "^19.1.1",
                         "clsx":  "^2.1.1"
                     },
    "devDependencies":  {
                            "autoprefixer":  "^10.4.21",
                            "postcss":  "^8.5.6",
                            "tailwindcss":  "^4.1.13",
                            "tsup":  "^8.5.0",
                            "typescript":  "^5.9.2",
                            "@types/react":  "^19.0.0",
                            "@types/react-dom":  "^19.0.0"
                        },
    "peerDependencies":  {
                             "react":  "\u003e=18",
                             "react-dom":  "\u003e=18"
                         }
}
